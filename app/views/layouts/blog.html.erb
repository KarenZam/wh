    <![endif]-->
    <script src="/js/custom.modernizr.js"></script>
  </head>
  <body>
    <div id="preloader">
      <div id="status">&nbsp;</div>
    </div>

    <div id="header">
      <div class="col-sm-12 col-lg-12">
        <div class="row">
          <div id="logo">
            <a class="scroll" href="index.html"><img src="img/logo.png" alt="" /></a>
          </div>
          <ul id="menu">
            <li><a class="scroll" href="index.html">Home</a></li>
            <li><a class="scroll" href="blog.html">Blog</a></li>
            <li><a class="scroll" href="features.html">Features</a></li>
          </ul>
          <div id="nav-toggle">
            <i class="typcn typcn-th-menu"></i>
          </div>
        </div>
      </div>
    </div>

    <section id="blog">
      <div class="container pad90">
        <div class="row">
          <div class="text-center col-sm-12 col-lg-12">
            <h1>Blog</h1>
            <div class="lead">
              It's the little details that are vital.
              <br/>
              Little things make big things happen.
            </div>
          </div>
        </div>

        <%= yield %>

      </div>

      <div class="col-sm-12 col-lg-12">
        <div class="row">
          <div class="pager text-center">
            <a href="#"><i class="typcn typcn-arrow-left-outline"></i></a>
            back : next
            <a href="#"><i class="typcn typcn-arrow-right-outline"></i></a>
          </div>
        </div>
      </div>
    </section>

    <footer>
      <div class="center footer">
        <a href="#home" class="scroll"><i class="typcn typcn-arrow-up-outline"></i></a>
        <br>
        <div id="copyright" class="bouncein">
          &copy; 2013 Urban Space Inc.
          <br>
          <a href="http://www.spiralpixel.com">Template by Spiral Pixel</a>
        </div>
      </div>
    </footer>

    <script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="//maps.google.com/maps/api/js?sensor=false&amp;language=en"></script>
    <%= javascript_include_tag "blog" %>
    <%= render partial: "layouts/messages" %>
  </body>
</html>
