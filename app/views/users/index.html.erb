<div class="row">
  <div class="col-md-12">
    <h1>Users</h1>

    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>Email</th>
          <th>Type</th>
          <th>Admin?</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.email %></td>
            <td><%= user.user_category %></td>
            <td><%= user.is_admin ? "Yes" : "No" %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>