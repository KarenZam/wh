<div class="row">
  <div class="span12">
    <h1>Users</h1>

    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Startup?</th>
          <th>Admin?</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.is_startup ? "Yes" : "No" %></td>
            <td><%= user.email ? "Yes" : "No" %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>